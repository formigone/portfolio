attribute vec3 aVertexPosition;uniform mat4 uMVMatrix;uniform mat4 uPMatrix;varying vec4 vFinalColor;void main(void){    vec4 finalPos = uPMatrix * uMVMatrix * vec4(aVertexPosition, 1.0);    gl_Position = finalPos;    vec4 fog_color = vec4(0.13, 0.13, 0.13, 1.0);    vec4 vertex_color = vec4(1.0, 1.0, 1.0, 1.0);    float fog;    float fog_coord;    float fog_end = 50.0;    fog_coord = abs(finalPos.z);    fog_coord = clamp(fog_coord, 0.0, fog_end);    fog = (fog_end - fog_coord) / fog_end;    fog = clamp(fog, 0.0, 1.0);    vFinalColor = mix(fog_color, vertex_color, fog);}